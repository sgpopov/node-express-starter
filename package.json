{
  "name": "node-express-starter",
  "version": "1.0.0",
  "description": "A starter project for building RESTful APIs using Node.js",
  "main": "src/app.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p .",
    "start:dev": "cross-env NODE_ENV=development && node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:prod": "cross-env NODE_ENV=production && node dist/app.js",
    "start:watch": "nodemon",
    "lint": "eslint ./src/"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgpopov/node-express-starter.git"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express",
    "boilerplate",
    "starter"
  ],
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "http-status-codes": "^1.4.0",
    "http-terminator": "^2.0.3",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.7",
    "@types/http-terminator": "^2.0.1",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
